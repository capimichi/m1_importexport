<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>

<form id="import_form" method="post" enctype="multipart/form-data"
      action="<?php echo Mage::helper("adminhtml")->getUrl("importexport/import/ajax"); ?>">

    <input type="hidden" name="progress_file"
           value="<?php echo sys_get_temp_dir() . DIRECTORY_SEPARATOR . implode("_", [
                   'cmimport',
                   md5(rand(0, 999999999)),
                   ".json",
               ]); ?>">

    <label>
        Import
        <input type="file" name="file">
    </label>

    <input type="submit" value="Importa">
</form>

<script>
    $j = jQuery.noConflict();

    $j(function () {
        var $form = $j("#import_form");
        $form.on("submit", function (e) {
            var formData = new FormData(this);
            $j.ajax({
                url: window.location.pathname,
                type: 'POST',
                data: formData,
                success: function (response) {
                    alert(response);
                },
                cache: false,
                contentType: false,
                processData: false
            });
            e.preventDefault();
        });
    });

</script>
