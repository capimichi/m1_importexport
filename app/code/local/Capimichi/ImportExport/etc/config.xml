<?xml version="1.0"?>

<config>
    <modules>
        <Capimichi_ImportExport>
            <version>0.1.0</version>
        </Capimichi_ImportExport>
    </modules>


    <global>
        <helpers>
            <importexport>
                <class>Capimichi_ImportExport_Helper</class>
            </importexport>
        </helpers>
    </global>


    <admin>
        <routers>
            <importexport>
                <use>admin</use>
                <args>
                    <module>Capimichi_ImportExport</module>
                    <frontName>importexport</frontName>
                </args>
            </importexport>
        </routers>
    </admin>

    <adminhtml>
        <menu>
            <menu1 translate="title" module="ImportExport">
                <title>CM ImportExport</title>
                <sort_order>60</sort_order>
                <children>
                    <menuitem1 module="ImportExport">
                        <title>Import</title>
                        <action>importexport/import</action>
                    </menuitem1>
                    <menuitem2 module="ImportExport">
                        <title>Export</title>
                        <action>importexport/export</action>
                    </menuitem2>
                </children>
            </menu1>
        </menu>
